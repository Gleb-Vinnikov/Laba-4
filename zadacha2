#include <iostream>
using namespace std;

int stringAddiction(char* s)
{
	int sum = 0, number = 0, k = 1;
	for (int i = 0; i < strlen(s); i++)
	{
		if (isdigit(s[i]))
		{
			number = number * 10 + (s[i] - '0');
		}
		else
		{
			sum += k*number;
			if (s[i] == '+') k = 1;
			else k = -1;
			number = 0;
		}
	}
	sum += k*number;
	return sum;
}

int main()
{
	return 0;
}
